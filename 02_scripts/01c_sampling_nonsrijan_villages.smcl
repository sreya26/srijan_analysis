{txt}
{com}.         
.         forval i=1/`num_locations' {c -(}
{txt}  2{com}.                 local first_ob: word `i' of `first_obs'
{txt}  3{com}.                 local last_ob: word `i' of `last_obs'
{txt}  4{com}.                 local loc: word `i' of `locations'
{txt}  5{com}.                 
.                 forval j = `first_ob'/`last_ob' {c -(}
{txt}  6{com}.                         if srijan_completed[`j'] == 0 & nonsrijan_sampled[`j'] == 0 {c -(}
{txt}  7{com}.                                         local srijan_village = srijan_village_code[`j']
{txt}  8{com}.                                         local nonsrijan_village = neighbour_village_code[`j']
{txt}  9{com}.                                         replace srijan_completed = 1 if srijan_village_code == `srijan_village' // eliminate this SRIJAN village from the set for which sampling is needed
{txt} 10{com}.                                         replace nonsrijan_sampled = 1 if neighbour_village_code == `nonsrijan_village' // elimite this non-SRIJAN village from the set from which villages can be sampled
{txt} 11{com}.                                         replace in_sample = 1 in `j'
{txt} 12{com}.                         {c )-}
